{
  "name": "todayshistory",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --go",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prettier": "npx prettier --write \"src/**/*.{js,jsx,json,ts,tsx}\"",
    "i18n:extract": "npx i18next --config i18next-parser.config.js",
    "translation:ci": "npx i18next --config i18next-parser.config.ci.js",
    "eslint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "madge:circular": "npx madge --circular App.tsx",
    "prepare": "husky install",
    "tsc": "npx tsc --noEmit",
    "clean:watchman": "watchman watch-del-all",
    "clean:android": "cd android && ./gradlew clean",
    "pods": "cd ios && pod install && cd ..",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^4.6.3",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "~51.0.26",
    "expo-build-properties": "^0.12.5",
    "expo-dev-client": "~4.0.22",
    "expo-font": "^12.0.9",
    "expo-localization": "^15.0.3",
    "expo-splash-screen": "^0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "^3.0.7",
    "i18next": "^23.12.2",
    "moment": "^2.30.1",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-i18next": "^15.0.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-reanimated": "^3.15.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.5.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-webview": "^13.8.6",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-plugin-import": "^1.13.8",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.4",
    "i18next-parser": "^9.0.2",
    "lint-staged": "^15.2.8",
    "madge": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "bash -c tsc --noEmit",
      "eslint --ext .js,.jsx,.ts,.tsx --fix --cache",
      "prettier --write",
      "madge --circular  src/App.tsx"
    ]
  }
}
